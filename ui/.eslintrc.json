{
  "extends": [
    "standard",
    "plugin:vue/base",
    "plugin:vue/essential",
    "plugin:vue/strongly-recommended",
    "plugin:vue/recommended"
  ],
  "rules": {
    "no-return-assign": "off",
    "semi": [
      "error",
      "always"
    ],
    "no-unused-expressions": "off",
    "vue/name-property-casing": [
      "error",
      "kebab-case"
    ],
    "vue/require-default-prop": "off"
  },
  "env": {
    "browser": true,
    "mocha": true
  },
  "globals": {
    "expect": false,
    "RouterLink": false
  },
  "parserOptions": {
    "ecmaVersion": 2018
  }
}
