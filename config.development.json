{
  "port": 3000,
  "logging": "console",
  "cache": {
    "defaultTimeout": 600
  },
  "limits": {
    "maxBodyLength": 500000,
    "maxPayloadSize": "10mb",
    "rateLimiting": {
      "window": 1,
      "maxRequests": 1
    }
  },
  "auth": {
    "key": null
  },
  "storageStrategy": "FileStorageStrategy",
  "aws": {
    "s3": {
      "endpoint": null,
      "key": null,
      "secret": null,
      "bucket": null
    }
  },
  "firebase": {
    "bucket": null
  },
  "fileStorage": {
    "location": "./storage/"
  }
}